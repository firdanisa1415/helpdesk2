{"ast":null,"code":"import { Container, Draggable } from \"vue-smooth-dnd\";\nimport { applyDrag, generateItems } from \"./utils\";\nexport default {\n  name: \"Simple\",\n  components: {\n    Container: Container,\n    Draggable: Draggable\n  },\n  data: function data() {\n    return {\n      items: generateItems(50, function (i) {\n        return {\n          id: i,\n          data: \"Draggable \" + i\n        };\n      })\n    };\n  },\n  methods: {\n    onDrop: function onDrop(dropResult) {\n      this.items = applyDrag(this.items, dropResult);\n    }\n  }\n};","map":{"version":3,"sources":["D:\\Tugas\\vue-notus\\src\\views\\admin\\BoardPenugasan.vue"],"names":[],"mappings":"AAeA,SAAS,SAAS,EAAE,SAAQ,QAAS,gBAAgB;AACrD,SAAS,SAAS,EAAE,aAAY,QAAS,SAAS;AAClD,eAAe;EACb,IAAI,EAAE,QAAQ;EACd,UAAU,EAAE;IAAE,SAAS,EAAT,SAAS;IAAE,SAAQ,EAAR;EAAU,CAAC;EACpC,IAAI,kBAAG;IACL,OAAO;MACL,KAAK,EAAE,aAAa,CAAC,EAAE,EAAE,UAAA,CAAA;QAAA,OAAM;UAAE,EAAE,EAAE,CAAC;UAAE,IAAI,EAAE,YAAW,GAAI;QAAE,CAAC;MAAA,CAAC;IACnE,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,MAAM,kBAAC,UAAU,EAAE;MACjB,IAAI,CAAC,KAAI,GAAI,SAAS,CAAC,IAAI,CAAC,KAAK,EAAE,UAAU,CAAC;IAChD;EACF;AACF,CAAC","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"simple-page\">\r\n        <Container @drop=\"onDrop\">\r\n          <Draggable v-for=\"item in items\" :key=\"item.id\">\r\n            <div class=\"draggable-item\">\r\n              {{item.data}}\r\n            </div>\r\n          </Draggable>\r\n        </Container>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Container, Draggable } from \"vue-smooth-dnd\";\r\nimport { applyDrag, generateItems } from \"./utils\";\r\nexport default {\r\n  name: \"Simple\",\r\n  components: { Container, Draggable },\r\n  data() {\r\n    return {\r\n      items: generateItems(50, i => ({ id: i, data: \"Draggable \" + i }))\r\n    };\r\n  },\r\n  methods: {\r\n    onDrop(dropResult) {\r\n      this.items = applyDrag(this.items, dropResult);\r\n    }\r\n  }\r\n};\r\n</script